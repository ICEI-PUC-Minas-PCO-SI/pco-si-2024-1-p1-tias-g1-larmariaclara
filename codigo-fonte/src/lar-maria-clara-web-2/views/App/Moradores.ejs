<div id="ITEM_APP">
    <div class="f b bg-white hd70">
        <div class="f">
            <button class="button-white" onclick="Next('#ITEM_NEW', '#ITEM_APP', '.new-item')">Inserir</button>
            <button class="button-white ml10" onclick="toExcel()">Excel</button>
            <button class="button-white ml10" onclick="updateit('mo')">Editar</button>
            <button class="button-white ml10" onclick="removeit('moradores')">Excluir</button>
        </div>
        <div class="f">
            <input type="text" name="search" placeholder="Nome Fantasia" class="w-auto ml10">
            <button class=" ml10" onclick="mr_search('/res/read', 'moradores', pit('[name=search]').value(), Moradores)">Pesquisar</button>
        </div>
    </div>
    <div class="bg-white mt10">
        <table>
            <tr>
                <th><input type="checkbox"></th>
                <th>Nome</th>
                <th>Data de entrada</th>
                <th>Curador</th>
                <th>Idade</th>
            </tr>
        </table>
        <div class="column-2a f b ac mt15 fs0.8">
            <div class="p-item">
                <p>Página 1 de 1</p>
            </div>
            <div class="p-item f far">
                <a href="#">1</a>
                <a href="#" class="ml10">2</a>
                <a href="#" class="ml10">3</a>
                <a href="#" class="ml10">4</a>
            </div>
        </div>
    </div>
</div>
<div id="ITEM_NEW" class="h bg-white">
    <h3 class="h manager-item new-item">Cadastro de morador</h3>
    <h3 class="h manager-item update-item">Atualizar de morador</h3>
    <div class="form-group tc user-photo">
        <img src="/user.png" class="auto p-pic" id="photo" >
        <label for="moFoto">
            <span class="icon-camera camera"></span>
        </label>
        <input type="file" class="h" accept="image/png, image/jpg" name="moFoto" id="moFoto" onchange="photoView(event)" data-id="photo">
    </div>
    <div class="t2 f b mt20">
        <div class="form-group">
            <p>Nome</p>
            <input type="text" name="moNome">
        </div>
        <div class="form-group">
            <p>Rg</p>
            <input type="text" name="moRg">
        </div>
    </div>
    <div class="t2 f b mt10">
        <div class="form-group">
            <p>CPF</p>
            <input type="text" name="moCPF" oninput="CPFMask()">
        </div>
        <div class="form-group">
            <p>Data de nascimento</p>
            <input type="text" name="moAniversario">
        </div>
    </div>
    <div class="mt20">
        <h4>Endereço</h4>
    </div>
    <div class="form-group mt10">
        <p>Cep</p>
        <input type="text" name="moCep">
    </div>
    <div class="t2 f b mt10">
        <div class="form-group">
            <p>Lagradouro</p>
            <input type="text" name="moLogradouro">
        </div>
        <div class="form-group">
            <p>Telefone</p>
            <input type="text" name="moTelefone">
        </div>
    </div>
    <div class="form-group mt10">
        <p>Número</p>
        <input type="text" name="moNumero">
    </div>
    <div class="t2 f b mt10">
        <div class="form-group">
            <p>Complemento</p>
            <input type="text" name="moComplemento">
        </div>
        <div class="form-group">
            <p>Valor total beneficio</p>
            <input type="text" name="moBeneficioTotal">
        </div>
    </div>
    <div class="form-group mt10">
        <p>Bairro</p>
        <input type="text" name="moBairro">
    </div>
    <div class="t2 f b mt10">
        <div class="form-group">
            <p>Nome do Município</p>
            <input type="text" name="moMunicipio">
        </div>
        <div class="form-group">
            <p>Vencimento da mensalidade</p>
            <input type="text" name="moMensalidadeVencimento">
        </div>
    </div>
    <div class="form-group mt10">
        <p>Nome do Pais</p>
        <input type="text" name="moPais">
    </div>
    <div class="form-group mt10">
        <p>Data inclusão morador</p>
        <input type="text" name="moinclusaodata">
    </div>
    <div class="form-group mt10">
        <p>Observações</p>
        <textarea rows="8" name="moObservacoes"></textarea>
    </div>

    <div class="mt20">
        <h4># Curador</h4>
    </div>
    <div class="t2 f b mt10">
        <div class="form-group ">
            <p>Nome</p>
            <input type="text" name="moNomeCurador">
        </div>
        <div class="form-group ">
            <p>Telefone</p>
            <input type="text" name="moTelefoneCurador">
        </div>
    </div>
    <div class="mt20">
        <h4># Conta bancária</h4>
    </div>
    <div class="form-group mt10">
        <p>Conta</p>
        <input type="text" name="moConta">
    </div>
    <div class="t2 f b mt10">
        <div class="form-group ">
            <p>Agencia</p>
            <input type="text" name="moAgencia">
        </div>
        <div class="form-group ">
            <p>Banco</p>
            <input type="text" name="moBanco">
        </div>
    </div>

    <div class="form-group mt10 f b ac">
        <button class="busytype manager-item new-item" data-text="Enviar" onclick="mr('/res/create', 'moradores')">Enviar</button>
        <button class="busytype h manager-item update-item" data-text="Enviar" onclick="mr('/res/update', 'moradores')">Atualizar</button>
        <a href="#" onclick="Next('#ITEM_APP', '#ITEM_NEW')">Cancelar</a>
    </div>
</div>
<input type="hidden" name="prefix" value="mo">
<input type="hidden" name="moUpdated_hash">